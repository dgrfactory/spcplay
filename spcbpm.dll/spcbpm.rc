            VALUE "OriginalFilename", "spc_bpm.dll\0"
            VALUE "ProductName", "SPCBPM\0"
            VALUE "ProductVersion", "$CAP_PRODUCT_VER\0"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x0411, 0x03A4
    END
END
