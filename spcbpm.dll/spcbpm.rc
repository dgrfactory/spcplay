        BEGIN
            VALUE "FileDescription", "BPM Analyzer for SNESAPU\0"
            VALUE "FileVersion", "$CAP_FILE_VER\0"
            VALUE "LegalCopyright", "Copyright (C) 2023-$YEAR degrade-factory.\0"
